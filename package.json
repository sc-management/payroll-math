{
  "name": "@shaking-crab/payroll-math",
  "version": "0.1.0",
  "description": "Pure TypeScript payroll calculators for FE & BE",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "test": "vitest run",
    "dev": "vitest watch",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/node": "^24.8.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
